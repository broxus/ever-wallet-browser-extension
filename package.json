{
  "name": "sparx-wallet",
  "version": "1.3.0",
  "description": "SparX Wallet browser extension",
  "scripts": {
    "start": "webpack --mode=development --watch --stats-error-details --env beta",
    "build:prod": "webpack --mode=production",
    "build:beta": "webpack --mode=production --env beta",
    "release:prod": "npm run build:prod && npm run archive -- -v prod",
    "release:beta": "npm run build:beta && npm run archive -- -v beta",
    "release": "rm -rf release && npm run release:beta && npm run release:prod",
    "archive": "sh scripts/create-archive.sh",
    "generate-icons": "node scripts/generate-icons.js",
    "lint": "eslint ./src --ext .tsx,.ts --quiet --cache --cache-location ./node_modules/.cache/",
    "prepare": "husky install"
  },
